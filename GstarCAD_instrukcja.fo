                            <fo:block-container space-after="0" margin-left="0" margin-right="0" space-before="0"><fo:block end-indent="0" start-indent="0">
                                <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0"><fo:inline color="rgb(51 , 122 , 183)"><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_4">Interfejs
                                        użytkownika</fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_4"><fo:inline font-style="italic"><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_4"> (on page </fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_4"><fo:page-number-citation ref-id="unique_4"/></fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_4">)</fo:basic-link></fo:inline></fo:basic-link></fo:inline></fo:block></fo:block-container>
                                <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0"><fo:inline color="rgb(51 , 122 , 183)"><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_5">Dostosowanie
